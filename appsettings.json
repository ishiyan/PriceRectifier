{
  "Output": {
    "Folder": "output",
    "ClosingPriceOnly": true
  },
  "Topline": {
    "InputFolder": "input-topline",
    "ScalarInstrumentsCsv": "index-idc.csv",
    "OhlcvInstrumentsCsv": "index-idohlcv.csv",
    "ScalarDataCsv": "idc.csv",
    "OhlcvDataCsv": "idohlcv.csv"
  },
  "DataConsistency": {
    "CheckPositivePrice": true,
    "CheckOhlcvPositiveVolume": false,
    "CheckOhlcvPositiveOrZeroVolume": false,
    "CheckOhlcvPriceConsistency": false
  },
  "DataAlignment": {
    "FillMissingDatesInTheSet": true,
    "SelectLatestStartDateInTheSet": true,
    "SelectEarliestEndDateInTheSet": true,
    "StartDateInclusive": "",
    "EndDateInclusive": ""
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "PriceRectifier": "Trace"
      }
    },
    "Console": {
      "LogLevel": {
        "PriceRectifier": "Information"
      }
    },
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning",
      "PriceRectifier": "Trace"
    }
  }
}
